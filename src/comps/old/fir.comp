COMP(fir);

HAL_PIN(in) = 0.0;
HAL_PIN(out) = 0.0;
HAL_PIN(f0) = 0.0;
HAL_PIN(f1) = 0.0;
HAL_PIN(f2) = 0.0;
HAL_PIN(f3) = 0.0;

MEM(float in0) = 0.0;
MEM(float in1) = 0.0;
MEM(float in2) = 0.0;
MEM(float in3) = 0.0;

RT(
  in0 = PIN(in);
  PIN(out) = in0 * PIN(f0) + in1 * PIN(f1) + in2 * PIN(f2) + in3 * PIN(f3);
  in3 = in2;
  in2 = in1;
  in1 = in0;
);

ENDCOMP;
